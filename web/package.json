{
  "name": "rustzen-admin",
  "private": true,
  "version": "0.2.2",
  "type": "module",
  "scripts": {
    "prepare": "cd .. && husky web/.husky || true",
    "preview": "vite preview",
    "postbuild": "zipper -o dist-rustzen-admin",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,less,md}\"",
    "lint": "npm run lint:types && npm run lint:fix",
    "lint:types": "tsc -p tsconfig.json --noEmit",
    "lint:fix": "eslint --cache --fix ."
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/plots": "^2.6.5",
    "@ant-design/pro-components": "^2.8.10",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@emotion/react": "^11.14.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.133.36",
    "@tanstack/react-router-devtools": "^1.133.36",
    "@tanstack/router-plugin": "^1.133.36",
    "antd": "^5.27.4",
    "http-proxy-middleware": "^3.0.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@rustzen/zipper": "^0.1.3",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "npm:rolldown-vite@^7.1.16"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{css,less}": [
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "volta": {
    "node": "24.2.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    },
    "onlyBuiltDependencies": [
      "@rustzen/zipper",
      "@swc/core",
      "@tailwindcss/oxide",
      "esbuild"
    ]
  }
}
